<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="~/Content/img/logo/icon.png" type="image/x-icon" />

    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/ionicons.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/js/source/jquery.fancybox.css" rel="stylesheet" />
    <link href="~/Content/css/animations.min.css" rel="stylesheet" />
    <link href="~/Content/css/style-white.css" rel="stylesheet" />

    <script src="~/Content/js/jquery-1.11.1.js"></script>
    <script src="~/Content/js/bootstrap.js"></script>
    <script src="~/Content/js/vegas/jquery.vegas.min.js"></script>
    <script src="~/Content/js/appear.min.js"></script>
    <script src="~/Content/js/animations.min.js"></script>
    <script src="~/Content/js/jquery.validate.min.js"></script>
    <script src="~/Content/js/custom.js"></script>
</head>
<body>

    <!-- Header -->
    @Html.Partial("_Header")
    <!-- Body -->
    @RenderBody()
    <!-- Footer -->
    @Html.Partial("_Footer")
 
    @RenderSection("scripts", required: false)
</body>
</html>
